# Set up for Zsh aliases

alias python="python3"
alias code="code-insiders"

ju()
{
    if [ -z "$1" ]; then
        echo "Usage: upto [directory]"
        return
    fi
    local upto=$1
    cd "${PWD/\/$upto\/*//$upto}"
}

jd(){
    if [ -z "$1" ]; then
        echo "Usage: jd [directory]";
        return 1
    else
        dir=$(find . -type d -name $1 -print -quit)
        if [ -z $dir ]; then
            return 0
        else
            cd $dir
        fi
    fi
}

